{
  "name": "kubernetes-namespace-provisioner",
  "version": "1.0.0",
  "description": "TypeScript implementation of Kubernetes namespace provisioning API",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "eslint:fix": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --fix",
    "format": "prettier --write '**/*.{ts,js,json,md}'",
    "format:check": "prettier --check '**/*.{ts,js,json,md}'",
    "stylelint": "stylelint src/**/*.css",
    "stylelint:fix": "stylelint src/**/*.css --fix",
    "license:check": "$PWD/scripts/container_tool.sh run --rm -t -v $PWD/:/workspace/project quay.io/che-incubator/dash-licenses:next --check",
    "license:generate": "$PWD/scripts/container_tool.sh run --rm -t -v $PWD/:/workspace/project quay.io/che-incubator/dash-licenses:next",
    "header:check": "node .config/license-check.js",
    "header:fix": "node .config/license-check.js --fix",
    "pretest": "yarn header:check",
    "prebuild": "yarn header:check"
  },
  "keywords": [
    "kubernetes",
    "namespace",
    "provisioning",
    "api"
  ],
  "author": "",
  "license": "EPL-2.0",
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "@kubernetes/client-node": "^0.20.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.26.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.0",
    "@types/yamljs": "^0.2.34",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "glob": "^10.3.10",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
